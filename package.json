{
  "name": "paper-walrus-server",
  "version": "0.1.0",
  "description": "Server component of the paper-walrus project.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build-server": "babel -d dist src -s",
    "build": "npm run clean && npm run build-server",
    "start": "node dist/serve.js",
    "watch": "babel-watch -w src src/serve.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tehwalris/paper-walrus-server.git"
  },
  "keywords": [
    "papers",
    "handouts",
    "sorting",
    "storage",
    "database",
    "management",
    "searching"
  ],
  "author": "Philippe \"tehwalris\" Voinov",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/tehwalris/paper-walrus-server/issues"
  },
  "homepage": "https://github.com/tehwalris/paper-walrus-server#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-polyfill": "^6.20.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.15.2",
    "email-validator": "^1.0.7",
    "express": "^4.14.0",
    "express-graphql": "^0.6.1",
    "graphql": "^0.8.2",
    "graphql-relay": "^0.4.4",
    "jsonwebtoken": "^7.2.1",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "mime": "^1.3.4",
    "minio": "^2.0.3",
    "moment": "^2.17.1",
    "pg": "^6.1.2",
    "promisify-node": "^0.4.0",
    "sharp": "^0.17.0",
    "uuid": "^3.0.1"
  },
